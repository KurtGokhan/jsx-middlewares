{
  "name": "jsx-middlewares",
  "type": "module",
  "description": "Transform rendered JSX with middlewares",
  "homepage": "https://gkurt.com/jsx-middlewares/",
  "repository": {
    "type": "git",
    "url": "http://github.com/KurtGokhan/jsx-middlewares"
  },
  "keywords": ["jsx", "react", "middleware", "decorator", "directive"],
  "version": "1.0.2",
  "license": "MIT",
  "author": {
    "name": "Gokhan Kurt",
    "email": "krtgkn@gmail.com",
    "url": "https://github.com/KurtGokhan"
  },
  "files": ["README.md", "dist"],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./react": {
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js",
      "types": "./dist/esm/react/index.d.ts",
      "default": "./dist/esm/react/index.js"
    },
    "./react/*": {
      "import": "./dist/esm/react/*.js",
      "require": "./dist/cjs/react/*.js",
      "types": "./dist/esm/react/*.d.ts",
      "default": "./dist/esm/react/*.js"
    }
  },
  "scripts": {
    "tsc": "tsc -b tsconfig.lib.json tsconfig.cjs.json",
    "start": "npm run tsc -- --watch",
    "build": "npm run clean && npm run tsc",
    "clean": "rimraf dist && rimraf tsconfig.lib.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "biome lint",
    "format": "biome format",
    "check": "biome check",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.14",
    "@types/react": "19.0.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.1"
  },
  "lint-staged": {
    "*": ["biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"]
  },
  "workspaces": [".", "packages/*", "examples/*", "website"]
}
