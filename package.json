{
  "name": "jsx-middlewares",
  "description": "Transform rendered JSX with middlewares",
  "homepage": "https://gkurt.com/jsx-middlewares/",
  "repository": {
    "type": "git",
    "url": "http://github.com/KurtGokhan/jsx-middlewares"
  },
  "keywords": [
    "jsx",
    "react",
    "middleware",
    "decorator",
    "directive"
  ],
  "version": "1.0.2",
  "license": "MIT",
  "author": {
    "name": "Gokhan Kurt",
    "email": "krtgkn@gmail.com",
    "url": "https://github.com/KurtGokhan"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react/index.js",
    "./react/jsx-runtime": "./dist/react/jsx-runtime.js",
    "./react/jsx-dev-runtime": "./dist/react/jsx-dev-runtime.js",
    "./react/types": "./dist/react/types.d.ts"
  },
  "scripts": {
    "tsc": "tsc -p tsconfig.lib.json",
    "start": "npm run tsc -- --watch --incremental",
    "build": "npm run clean && npm run tsc",
    "clean": "rimraf dist && rimraf tsconfig.lib.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "biome lint",
    "format": "biome format",
    "check": "biome check",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.11",
    "@types/react": "18.2.47",
    "jest": "^29.7.0",
    "prettier": "^3.2.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.9.1"
  },
  "lint-staged": {
    "*": ["biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"]
  },
  "workspaces": ["packages/*", "examples/*", "website"]
}
